<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <title>My Data</title>
    <link rel="stylesheet" href="css/raw.css">
    <script src="../public/script.js"></script>
</head>

<body>

    <table>
        <thead>
            <tr>
                <td colspan="2">
                    <strong>relationships</strong>
                </td>
            </tr>
            <tr>
                <td>
                    <strong>list</strong>
                </td>
                <td class="grey">
                    _id
                </td>
                <td class="grey">
                    __v
                </td>
            </tr>
        </thead>
        <tbody>
            <% data.reverse().forEach(function(rel){ %>
            <tr>
                <td>
                    <ul id="dotless">
                        <% rel.list.forEach(function(interaction){ %>
                        <li>
                            <%= interaction %>
                        </li>
                        <% }); %>
                    </ul>
                </td>
                <td class="grey">
                    <%= rel._id %>
                </td>
                <td class="grey">
                    <%= rel.__v %>
                </td>
            </tr>
            <% }); %>
        </tbody>
    </table>

</body>

<footer>

</footer>

</html>